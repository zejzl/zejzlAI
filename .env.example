# ===============================
# AI Framework Environment Config
# ===============================

# -------- OpenAI / ChatGPT --------
OPENAI_API_KEY=sk-your-openai-key-here

# -------- Anthropic Claude --------
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# -------- Google Gemini --------
GEMINI_API_KEY=your-gemini-api-key-here

# -------- Zai AI --------
ZAI_API_KEY=your-zai-api-key-here

# -------- xAI Grok --------
GROK_API_KEY=your-grok-api-key-here

# -------- DeepSeek --------
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# -------- Alibaba Qwen --------
QWEN_API_KEY=your-qwen-api-key-here


# ===============================
# Persistence / Infrastructure
# ===============================

# Redis connection URL
REDIS_URL=redis://localhost:6379

# SQLite fallback database path
SQLITE_PATH=~/.ai_framework.db


# ===============================
# Payment Processing (Stripe)
# ===============================

# Stripe API keys (get from https://dashboard.stripe.com/apikeys)
STRIPE_API_KEY=sk_test_your-stripe-test-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-test-publishable-key

# Stripe webhook secret (get from Stripe dashboard webhook configuration)
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Webhook endpoint URL
STRIPE_WEBHOOK_URL=https://your-domain.com/api/subscriptions/webhook


# ===============================
# Security & Authentication
# ===============================

# API Keys for zejzl.net API access
# Format: ZEJZL_API_KEY_N=actual_key:tier:description
# Tiers: free (10 req/min), pro (100 req/min), enterprise (1000 req/min)

# Example API keys (generate your own using: python -c "import secrets; print('zejzl_' + secrets.token_urlsafe(24))")
ZEJZL_API_KEY_1=zejzl_your_random_key_here:pro:Main API Key
ZEJZL_API_KEY_2=zejzl_another_key_here:free:Development Key

# CORS allowed origins (comma-separated)
# Leave empty to use defaults (zejzl-net.vercel.app, zejzl.net)
CORS_ORIGINS=https://zejzl-net.vercel.app,https://zejzl.net,http://localhost:3000

# Environment (development or production)
ENVIRONMENT=production


# ===============================
# Optional Runtime Settings
# ===============================

# Default provider if none specified
DEFAULT_PROVIDER=chatgpt

# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
